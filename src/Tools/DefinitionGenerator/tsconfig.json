{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "lib": ["ES2019", "DOM"],
        "outDir": "../../../DefinitionGenerator",
        "sourceMap": false,
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true
    },
    "files": ["./entry.ts"]
}
